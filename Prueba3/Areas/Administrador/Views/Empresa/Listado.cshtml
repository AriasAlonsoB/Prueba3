@using BEViajaMas;
@{
    List<empresa> listadoEmpresa = (List<empresa>)ViewBag.ListadoEmpresa;
}

<table id="tbEmpresa" class="table table-striped">
    <thead>
        <tr>
            <th>Código</th>
            <th>Nombre</th>
            <th>RUC</th>
            <th>Fecha de Registro</th>
            <th>Dirección</th>
            <th>Rubro</th>
            <th>Telefono</th>
            <th>Detalle</th>
            <th>Estado</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (empresa item in listadoEmpresa)
        {
            <tr>
                <td>@item.id_empresa</td>
                <td>@item.nombre</td>
                <td>@item.ruc</td>
                <td>@item.fecha_registro</td>
                <td>@item.direccion</td>
                <td>@item.rubro</td>
                <td>@item.telefono</td>
                <td>@item.detalle</td>
                <td>@item.estado</td>
                <!--<td>
                    <input type="button" id="btnEliminarEmpresa" data-codigo="@item.id_empresa"
                           class="btnEliminarEmpresa btn btn-danger" value="Eliminar" />
                    <input type="button" id="btnEditarEmpresa" data-codigo="@item.id_empresa"
                           class="btnEditarEmpresa btn btn-info" value="Editar" />
                </td>-->
            </tr>
        }
    </tbody>

</table>

